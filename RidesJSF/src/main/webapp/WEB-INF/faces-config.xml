<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>mainBean</managed-bean-name>
		<managed-bean-class>eredua.bean.MainBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>Mainpage.xhtml</display-name>
		<from-view-id>/Mainpage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createRide</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Mainpage.xhtml</display-name>
		<from-view-id>/Mainpage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>home.xhtml</display-name>
		<from-view-id>/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Login</from-outcome>
			<to-view-id>/LogIn.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>home.xhtml</display-name>
		<from-view-id>/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>LogIn.xhtml</display-name>
		<from-view-id>/LogIn.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Main</from-outcome>
			<to-view-id>/Mainpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SelectOne.xhtml</display-name>
		<from-view-id>/SelectOne.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewRides</from-outcome>
			<to-view-id>/ViewRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Mainpage.xhtml</display-name>
		<from-view-id>/Mainpage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>selectOne</from-outcome>
			<to-view-id>/SelectOne.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Mainpage</from-outcome>
			<to-view-id>/Mainpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Mainpage</from-outcome>
			<to-view-id>/Mainpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ViewRides.xhtml</display-name>
		<from-view-id>/ViewRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Mainpage</from-outcome>
			<to-view-id>/Mainpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>
